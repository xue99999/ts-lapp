} */

.view-bu {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.view-bu view {
  width: 33.3%;
}

.bushu {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}

.bushu image {
  width: 40rpx;
  height: 40rpx;
  margin: 0 10rpx;
}
.tupian {
  display: flex;
  flex-direction: row-reverse;
  margin-right: 40rpx;
}

.tupian image {
  width: 70rpx;
  height: 70rpx;
}

