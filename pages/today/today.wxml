	
<view class='box'>

  <view class='header'>
		<view wx:for='{{date}}'wx:key class='{{(index == todayIndex) && isTodayWeek ? "weekMark" : ""}}'>{{item}}</view>

	</view>
  <view class='headers'>
     <view wx:for='{{days}}' wx:key data-index="{{item}}" class='{{ item == today? "weekMark" : ""}}' bindtap='clickArr'>{{item}}            </view>
  </view>

  <view class='view-tishi'>
        <view bindtap='clickjinri' class='view-jinr'>今日提示</view>
  </view> 
  <view class='view-img'>
        <image src='../img/椭圆37@3x.png'></image>
         <!-- <view>{{item.inner}}</view>
        <view>{{item.userModel}}</view>  -->
        <view class='view-inner' wx:if="{{userModel==='02'}}">
              <view>{{showObj['02'].top}}</view>
              <view>{{showObj['02'].middle}}</view> 
              <view class='view-last'>{{showObj['02'].shouyun}}</view>      
        </view>

         <view class='view-inner' wx:if="{{userModel==='01'}}">
              <image src='{{bobyImg}}'></image>
              <view>{{showObj['01'].babyText}}</view>
               <view class='view-last'>{{showObj['01'].lastText}}</view>
        </view>
        <view class='view-btn'>

        </view> 
  </view>

  <view class='view-bu'>
        <view></view>
        <view class='bushu' wx:for='{{recordsImg}}'wx:key >
              <image src='{{item}}'></image>
              <!-- <text>16 步数</text> -->
        </view> 
        <view class='tupian'>
              <image src='../img/add@3x.png' bindtap='navState'></image>
        </view>
  </view>


</view>