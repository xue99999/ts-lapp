/* pages/home/home.wxss */

.heads {
  width: 100%;
  position: fixed;
  height: 100%;
  /* height: 1333rpx; */
  background: #FE6F71;
  padding-top: 63px;
}

.imgs {
display: flex;
align-items: center;
flex-direction: column;
  margin-top: 150rpx;
}
.avatar{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.nickname{
  text-align: center;
}
.userinfo-avatar {
  overflow:hidden;
  display: block;
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx;
  margin-top: 50rpx;
  border-radius: 50%;
  border: 2px solid #fff;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}
.userinfo-name{
  color: white;
}
