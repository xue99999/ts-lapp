} */

.texts {
  margin-top: 108rpx;
  text-align: center;
  color: white;
}

.texts text {
  font-size: 124%;
}

.images {
  margin-top: 127rpx;
  width: 100%;
  display: flex;
}

.images>view {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.images_menst, .images_mom {
  margin-top: 300rpx;
  width: 352rpx;
  height: 80rpx;
  background: #fd918d;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.images_menst image {
  width: 53.7rpx;
  height: 71.8rpx;
}

.images_mom image {
  width: 41.6rpx;
  height: 71.8rpx;
}

.images view text {
  margin-top: 15rpx;
  color: white;
  font-size: 60%;
  letter-spacing: 3.3rpx;
}
