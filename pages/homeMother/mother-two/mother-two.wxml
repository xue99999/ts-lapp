<view>
      <view class='view-buzou'>
        <text>步骤 2   共有6步骤</text>
        <image src='../../img/buzhou2.png'></image>
      </view>
      <view class='view-tes'>一般上, 您的月经持续多久?</view>
      <!-- 选择器 -->
      <scroll-view style="height: 550rpx;" scroll-y class='scroll-array'>
          <view wx:for='{{array}}' wx:key class='view-array'>{{item}}</view>
      </scroll-view>

      <view class='view-fot'>
          <view class='fot-one'>
                <view class='box'>
                  <view  class='box_list' data-current="{{index}}"bindtap="chooseImg">
                      <image class="box-image" src="{{show==false ? imgUrl : curUrl}}"></image>
                  </view>
                </view>
                <text>我不记得了</text>      
          </view>
          <button class='btn' bindtap='navto'>下一步</button>
      </view>
</view>